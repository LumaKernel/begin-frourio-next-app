{
  "name": "frourio-next-app",
  "version": "0.0.0",
  "description": "Begin frourio Next.js app",
  "arc": {
    "app": "frourio-next-app",
    "http": [
      {
        "/api/*": {
          "method": "any",
          "src": "/frourio-handler"
        }
      }
    ],
    "tables": [
      {
        "data": {
          "scopeID": "*String",
          "dataID": "**String",
          "ttl": "TTL"
        }
      }
    ],
    "static": {
      "folder": "out",
      "spa": true
    }
  },
  "scripts": {
    "start": "run-p start:*",
    "start:api": "cross-env NODE_ENV=testing ARC_LOCAL=1 npx sandbox",
    "start:aspida": "aspida -w",
    "start:frourio": "frourio-express -w",
    "start:handler": "npm run build:handler -- --watch",
    "start:next": "next dev",
    "lint": "eslint . --ext .js,.ts,.tsx",
    "fix": "npm run lint -- --fix",
    "typecheck": "tsc --noEmit",
    "prebuild": "frourio-express && aspida",
    "build": "run-s build:*",
    "build:next": "next build && next export",
    "build:handler": "npx esbuild frourio-handler/index.ts --tsconfig=tsconfig-handler.json --bundle --platform=node --outfile=frourio-handler/index.js",
    "pretest": "frourio-express && aspida",
    "test": "run-s test:*",
    "test:typecheck": "npm run typecheck"
  },
  "dependencies": {
    "@vendia/serverless-express": "^4.3.9",
    "class-validator": "^0.13.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "hasha": "^5.2.2",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2"
  },
  "devDependencies": {
    "@architect/functions": "^3.13.9",
    "@architect/sandbox": "^3.4.0",
    "@aspida/axios": "^1.5.0",
    "@begin/data": "^2.0.1",
    "@testing-library/react": "^11.2.5",
    "@types/architect__functions": "^3.13.6",
    "@types/begin__data": "^2.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-jwt": "^6.0.2",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^14.14.31",
    "@types/react": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "axios": "^0.21.1",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "esbuild": "^0.12.15",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "frourio-express": "^0.25.1",
    "next": "^10.0.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "ts-loader": "^8.0.17",
    "typescript": "~4.2"
  }
}
